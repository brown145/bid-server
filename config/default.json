{
  "host": "localhost",
  "port": 3333,
  "public": "../public/",
  "paginate": {
    "default": 100,
    "max": 100
  },
  "nedb": "NEDB_PATH",
  "authentication": {
    "secret": "AUTH_SECRET",
    "strategies": [
      "jwt"
    ],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "todo": "TODO",
      "audience": "https://bid-server.todo.com",
      "subject": "loginStatus",
      "issuer": "bid-server",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "google": {
      "clientID": "GOOGLE_SIGNIN_CLIENT_ID",
      "clientSecret": "GOOGLE_SIGNIN_CLIENT_SECRET",
      "successRedirect": "http://localhost:4444/",
      "scope": [
        "profile openid email"
      ]
    },
    "cookie": {
      "enabled": true,
      "name": "feathers-jwt",
      "httpOnly": false,
      "secure": false
    }
  }
}
